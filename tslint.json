{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "jsx-no-multiline-js": false,
        "jsx-wrap-multiline": true,
        "trailing-comma": true,
        "jsx-curly-spacing": "always",
        "jsx-boolean-value": false,
        "jsx-no-lambda": false
    }
}
